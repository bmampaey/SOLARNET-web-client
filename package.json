{
  "name": "svo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@adapttive/vue-markdown": "^4.0.1",
    "axios": "^0.27.2",
    "bootstrap-vue": "^2.22.0",
    "core-js": "^3.22.8",
    "jsonp-promise": "^0.1.2",
    "vue": "^2.6.14",
    "vue-router": "^3.5.4"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.12.1",
    "@vue/cli-plugin-babel": "^5.0.4",
    "@vue/cli-plugin-eslint": "^5.0.4",
    "@vue/cli-plugin-router": "^5.0.4",
    "@vue/cli-service": "^5.0.4",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^4.6.1",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-vue": "^6.2.2",
    "mutationobserver-shim": "^0.3.3",
    "patch-package": "^6.4.7",
    "popper.js": "^1.16.0",
    "portal-vue": "^2.1.6",
    "prettier": "^1.19.1",
    "sass": "^1.51.0",
    "sass-loader": "^8.0.2",
    "vue-cli-plugin-bootstrap-vue": "^0.7.0",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended",
      "plugin:prettier/recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "indent": [
        "error",
        "tab"
      ],
      "prettier/prettier": [
        "warn",
        {
          "useTabs": true,
          "singleQuote": true,
          "semi": true,
          "printWidth": 200
        }
      ],
      "vue/no-unused-components": "warn",
      "vue/no-unused-vars": "warn",
      "no-console": "warn",
      "no-debugger": "warn",
      "vue/array-bracket-spacing": "warn",
      "vue/arrow-spacing": "warn",
      "vue/block-spacing": "warn",
      "vue/camelcase": "warn",
      "object-shorthand": [
        "error",
        "methods"
      ]
    }
  },
  "babel": {
    "presets": [
      [
        "@vue/cli-plugin-babel/preset",
        {
          "useBuiltIns": "usage"
        }
      ]
    ]
  },
  "browserslist": [
    "defaults"
  ]
}
